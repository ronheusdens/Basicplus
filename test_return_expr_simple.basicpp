REM Simple test of procedure return value expressions
PROCEDURE Add(X, Y)
    LET result = X + Y
    RETURN result
END PROCEDURE

REM Test 1: Assign result to variable
LET A = Add(3, 5)
PRINT "A =", A

REM Test 2: Use in expression
LET B = Add(10, 20) * 2
PRINT "B =", B

REM Test 3: Use in IF condition
IF Add(2, 3) > 4 THEN
    PRINT "Test 3 passed"
ENDIF

END
