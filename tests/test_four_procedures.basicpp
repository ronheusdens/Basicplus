CLASS Test(A, B, C, D)
    PROCEDURE Method1()
        LET A = A + 1
        RETURN A
    END PROCEDURE
    PROCEDURE Method2()
        LET B = B + 1
        RETURN B
    END PROCEDURE
    PROCEDURE Method3()
        LET C = C + 1
        RETURN C
    END PROCEDURE
    PROCEDURE Method4()
        RETURN D
    END PROCEDURE
END CLASS

LET T = NEW Test(1, 2, 3, 4)
PRINT T.Method1()
PRINT T.Method4()
END
