CLASS Circle(Radius)
    PROCEDURE Circumference()
        RETURN 2 * 3.14159 * Radius
    END PROCEDURE
    
    PROCEDURE GetRadius()
        RETURN Radius
    END PROCEDURE
END CLASS

LET C = NEW Circle(5)
PRINT "Circle circumference:", C.Circumference()
PRINT "Circle radius:", C.GetRadius()

PRINT "Phase 2 Classes Test Complete"
END