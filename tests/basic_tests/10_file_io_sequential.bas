10 REM SEQUENTIAL FILE READ/WRITE
20 PRINT "SEQUENTIAL I/O"
30 REM Write sequential data
40 OPEN "seq.txt" FOR OUTPUT AS #1
50 FOR I = 1 TO 5
60 PRINT #1, I * 10
70 NEXT I
80 CLOSE #1
90 REM Read sequential data
100 OPEN "seq.txt" FOR INPUT AS #1
110 FOR I = 1 TO 5
120 INPUT #1, V
130 PRINT V
140 NEXT I
150 CLOSE #1
160 PRINT "OK"
170 END
