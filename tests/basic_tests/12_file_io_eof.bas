10 REM EOF DETECTION
20 PRINT "EOF TEST"
30 REM Write test file
40 OPEN "eof_test.txt" FOR OUTPUT AS #1
50 PRINT #1, 1
60 PRINT #1, 2
70 PRINT #1, 3
80 CLOSE #1
90 REM Read and detect EOF
100 OPEN "eof_test.txt" FOR INPUT AS #1
110 LET COUNT = 0
120 IF EOF(1) THEN GOTO 170
130 INPUT #1, V
140 LET COUNT = COUNT + 1
150 PRINT V
160 GOTO 120
170 CLOSE #1
180 PRINT "Read " COUNT " values"
190 PRINT "OK"
200 END
