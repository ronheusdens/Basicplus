10 REM COMPREHENSIVE MULTI-DIM ARRAY TEST
20 DIM A(2,2,2), B(2,2,2), C(2,2,2)
30 FOR K=0 TO 2
40 FOR I=0 TO 2
50 FOR J=0 TO 2
60 A(I,J,K)=I*100+J*10+K
70 B(I,J,K)=I+J+K
80 C(I,J,K)=A(I,J,K)+B(I,J,K)
90 NEXT J,I,K
100 REM PRINT RESULT
110 FOR K=0 TO 2
120 FOR I=0 TO 2
130 FOR J=0 TO 2
140 PRINT C(I,J,K),
150 NEXT J:PRINT
160 NEXT I:PRINT
170 NEXT K
180 END
