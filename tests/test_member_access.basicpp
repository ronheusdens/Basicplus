' Phase 2: Member Access Using Dot Notation

' Simple class with methods
CLASS Calculator(Value)
    PROCEDURE Add(X)
        LET Value = Value + X
        RETURN Value
    END PROCEDURE
    
    PROCEDURE Multiply(X)
        LET Value = Value * X
        RETURN Value
    END PROCEDURE
    
    PROCEDURE GetValue()
        RETURN Value
    END PROCEDURE
END CLASS

' Create instances
LET C1 = NEW Calculator(10)
LET C2 = NEW Calculator(5)

' Use member access with dot notation
PRINT "C1 initial value:", C1.GetValue()
LET RESULT1 = C1.Add(5)
PRINT "C1 after Add(5):", RESULT1
LET RESULT2 = C1.Multiply(2)
PRINT "C1 after Multiply(2):", RESULT2

PRINT "C2 initial value:", C2.GetValue()
LET RESULT3 = C2.Add(3)
PRINT "C2 after Add(3):", RESULT3

PRINT "Dot notation test complete"
END
